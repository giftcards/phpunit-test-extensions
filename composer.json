{
    "name": "giftcards/phpunit-test-extensions",
    "type": "library",
    "description": "A collection of GiftCards.com developed test extensions",
    "keywords": [],
    "homepage": "https://github.com/giftcards/phpunit-test-extension",
    "license": "MIT",
    "require": {
        "php": ">=5.3.3",
        "giftcards/phpunit-test-extension": "~1.0@dev",
        "fzaninotto/faker" : "~1.3",
        "symfony/form" : "2.4.*@dev",
        "symfony/event-dispatcher": "2.4.*@dev"
    },
    "require-dev": {
        "mockery/mockery" : ">=0.9.0@dev"
    },
    "autoload": {
        "psr-0": {
            "": ["lib/", "tests/"]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "suggest": {
        "fzaninotto/faker": "Necessary to utilize the FakerExtensions",
        "symfony/form": "Necessary to utilize the FormType Extension",
        "symfony/event-dispatcher": "Necessary to utilize the Utility Extension"
    }
}